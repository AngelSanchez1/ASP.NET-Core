@*
@model List<ObjetoEscuelaBase>
*@

<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col"> Nombre </th>
            <th scope="col"> Id </th>
            <th scope="col">Dirección</th>
            <th scope="col">Tipo de jornada</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.Nombre</td>
                <td>@obj.Id</td>
                <td>@obj.Dirección</td>
                <td>@obj.Jornada</td>
            </tr>
        }
    </tbody>
</table>